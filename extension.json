{
    "name": "SemanticGenealogy",
    "version": "0.3.10",
    "author": [
		"Tpt",
        "Thibault Taillandier <thibault@taillandier.name>"
    ],
    "url": "https://www.mediawiki.org/wiki/Extension:Semantic_Genealogy",
    "descriptionmsg": "semanticgenealogy-desc",
    "license-name": "GPL-2.0+",
    "type": "specialpage",
    "requires": {
        "MediaWiki": ">= 1.23.0",
		"extensions": {
			"SemanticMediaWiki": ">= 3.0"
		}
    },
    "MessagesDirs": {
        "SemanticGenealogy": [
            "i18n"
        ]
    },
    "SpecialPages": {
        "FamilyTree" : "SemanticGenealogy\\SpecialFamilyTree",
        "ImportGenealogyPages" : "SemanticGenealogy\\SpecialImportPages"
    },
    "ExtensionMessagesFiles": {
       "SemanticGenealogy": "SemanticGenealogy.i18n.php",
       "SemanticGenealogyAliases": "SemanticGenealogy.alias.php",
       "SemanticGenealogyNamespaces": "SemanticGenealogy.namespaces.php"
    },
    "callback": "SemanticGenealogy\\SemanticGenealogy::initExtension",
    "Hooks": {
      "@SkinBuildSidebar" : [
        "SemanticGenealogy\\Sidebar::addGenealogySideBar"
      ],
      "ParserBeforeTidy": [
        "SemanticGenealogy\\SemanticGenealogy::addExtensionCSS"
      ],
      "BeforePageDisplay": [
        "SemanticGenealogy\\SemanticGenealogy::onBeforePageDisplay"
      ]
    },
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"@remoteExtPath": "SemanticGenealogy",
        "styles": [
            "modules/styles.css"
        ]
	},
	"ResourceModules": {
	  "ext.smg.specialfamilytree": {
        "@remoteExtPath": "SemanticGenealogy",
        "styles": [
            "modules/styles.css"
        ]
	  }
	},
    "load_composer_autoloader": true,
    "manifest_version": 1
}
